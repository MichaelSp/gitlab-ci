= form_for @test_report do |f|
  - if @test_report.errors.any?
    #error_explanation
      %h2= "#{pluralize(@test_report.errors.count, "error")} prohibited this test_report from being saved:"
      %ul
        - @test_report.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :filename
    = f.text_field :filename
  .field
    = f.label :content
    = f.text_field :content
  .actions
    = f.submit 'Save'
